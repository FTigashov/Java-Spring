<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="cameraRoll" class="example_photograph.ColorCameraRoll"/>

    <bean id="camera" class="example_photograph_with_spring.Cameralmpl">
        <property name="cameraRoll" ref="cameraRoll"/>
    </bean>

    <bean id="assistant" class="mvc_patient_and_hospital.Assistant"/>
</beans>
